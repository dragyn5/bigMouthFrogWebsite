<%= form_for :proposal, url: proposals_path do |f| %>

  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :email %><br>
    <%= f.text_area :email %>
  </p>

    <p>
    <%= f.label :company %><br>
    <%= f.text_area :company %>
  </p>
    <p>
    <%= f.label :website_url %><br>
    <%= f.text_area :website_url %>
  </p>
    <p>
    <%= f.label :phone %><br>
    <%= f.text_area :phone %>
  </p>
    <p>
    <%= f.label :address %><br>
    <%= f.text_area :address %>
  </p>
    <p>
    <%= f.label :city %><br>
    <%= f.text_area :city %>
  </p>
    <p>
    <%= f.label :state %><br>
    <%= f.text_area :state %>
  </p>
    <p>
    <%= f.label :zip %><br>
    <%= f.text_area :zip %>
  </p>

    <p>
    <%= f.label :website_goals %><br>
    <%= f.text_area :website_goals %>
  </p>
        
  <p>
    <%= f.submit %>
  </p>
<% end %>